# KEY PAIR
algorithm = "RSA"
rsa_bits  = 4096
key = "Postgres/Postgres"
key_name= "Postgres"
bucket_name= "dev-ec2-key-pairs"

#SECURITY GROUP
sg-name= "Postgres-sg"
